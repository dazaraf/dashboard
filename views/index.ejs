<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
        <!-- Libraries -->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.7/angular.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/core.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>

  </head>
  <body ng-app = 'app' ng-controller = 'PnLController'>
  	
        <div class="row">
          <div class="col-md-6">
            <h4>Portfolios</h4>
            <div class="form-group" id = 'checkboxForm'>
              <label ng-repeat="name in portfolioNames" value="{{name}}" >
                <input type="checkbox" value="{{name}}" ng-checked="selection.indexOf(name) > -1" ng-click="toggleSelection(name)" class="checkbox-inline" > {{name}}
                </input>
                <br></br>
            </div>
            <p>{{updatedIncomingpnl}}</p>
          </div>
	    
	<p>Outside div {{updatedIncomingpnl}}</p>
  	<!--Angular script-->
  	<script src="../static/javascripts/frontend.js"></script>
  </body>
</html>
